{
	"name": "cornerstone",
	"version": "4.0.1",
	"description": "Site specific mu-plugin",
	"author": "Bob Moore <bob.moore@midwestfamilymadison.com>",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"mu-plugin",
		"project",
		"wordpress"
	],
	"homepage": "https://www.midwestfamilymadison.com",
	"repository": "https://github.com/MDMDevOps/{PROJECT}",
	"bugs": "https://github.com/MDMDevOps/{PROJECT}/issues",
	"dependencies": {
		"@10up/block-components": "^1.17.3",
		"@wordpress/dom-ready": "^3.46.0",
		"@wordpress/stylelint-config": "^21.29.0",
		"copy-webpack-plugin": "^11.0.0",
		"dompurify": "^3.0.3",
		"html-react-parser": "^4.0.0",
		"lodash.isequal": "^4.5.0",
		"mini-css-extract-plugin": "^2.7.6",
		"react-monaco-editor": "^0.52.0",
		"styled-components": "^6.0.5"
	},
	"devDependencies": {
		"@wordpress/block-editor": "^12.14.0",
		"@wordpress/blocks": "^12.23.0",
		"@wordpress/components": "^25.12.0",
		"@wordpress/element": "^5.23.0",
		"@wordpress/i18n": "^4.46.0",
		"@wordpress/scripts": "^26.17.0",
		"autoprefixer": "^10.4.14",
		"clean-webpack-plugin": "^4.0.0",
		"cssnano": "^6.0.1",
		"postcss": "^8.4.24",
		"postcss-assets": "^6.0.0",
		"postcss-nested": "^6.0.1",
		"postcss-pxtorem": "^6.0.0",
		"pxtorem": "^1.0.0",
		"sass": "^1.63.6"
	},
	"scripts": {
		"build": "npm run lint:css && npm run lint:js && ( npm run assets:build & npm run blocks:build )",
		"build:watch": "npm run assets:watch & npm run blocks:watch",
		"assets:build": "wp-scripts build --webpack-src-dir=./",
		"assets:hot": "wp-scripts start --hot --webpack-src-dir=./",
		"assets:watch": "wp-scripts start --webpack-src-dir=./",
		"blocks:build": "wp-scripts build --webpack-src-dir=./blocks --webpack-copy-php --config blocks.config.js",
		"blocks:hot": "wp-scripts start --hot --webpack-src-dir=./blocks --webpack-copy-php --config blocks.config.js",
		"blocks:watch": "wp-scripts start --webpack-src-dir=./blocks --webpack-copy-php --config blocks.config.js",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "wp-scripts check-licenses --prod --gpl2",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style './**/*.scss'",
		"fix:css": "wp-scripts lint-style './**/*.scss' --fix",
		"lint:js": "wp-scripts lint-js",
		"fix:js": "wp-scripts lint-js --fix",
		"lint:md:docs": "wp-scripts lint-md-docs",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"test:e2e": "wp-scripts test-e2e",
		"test:unit": "wp-scripts test-unit-js"
	},
	"browserslist": [
		"last 3 version",
		"> 1%",
		"not dead"
	]
}
